}

/*Controls the size and spacing of my header */
.header {
  padding: 30px;
  font-size: 40px;
  text-align: center;
  background: #E6F9FA;
}

/* Creates two columns where one of which takes up 3/4 of the page, with a sidebar comprised of 1/4 */
/* 3/4 */
.leftcolumn {   
  float: left;
  width: 75%;
}

/* 1/4 */
.rightcolumn {
  float: left;
  width: 25%;
  padding-left: 20px;
}



/* Creates individualized plates for each article */
.card {
   background-color: #E6F9FA;
   padding: 20px;
   margin-top: 20px;
}

.row:after {
  content: "";
  display: table;
  clear: both;
}


/* This is called a responsive layout which adapts to the size of the screen so if it is less than a certain amount of pixels, it will stack the left and right columns instead of squeezing them together */
@media screen and (max-width: 800px) {
  .leftcolumn, .rightcolumn {   
    width: 100%;
    padding: 0;
  }
}
</style>
